@model IList<float>

<p>Avarage speed</p>
@for (var i = 0; i < Model.Count; i++)
{
    var firstRound = i == 0;
    <div class="container">
        <span>
            @(firstRound 
                ? "Round 1: "
                : $"Rounds 1-{i + 1}")
        </span>
        <span>@Model[i] km/h</span>
    </div>
}